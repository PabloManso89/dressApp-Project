{
  "name": "seed-project",
  "version": "0.0.0",
  "private": true,
  "angular-cli": {},
  "scripts": {
    "build": "ng build",
    "eslint": "gulp eslint",
    "e2e": "protractor",
    "ng": "ng",
    "lint": "gulp tslint && gulp eslint",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "start": "ng serve & node app.js",
    "start-dev": "concurrently -k \"ng build --watch\" \"gulp serve\"",
    "start:es": "ng serve --aot --i18nFile=src/client/i18n/texts.es.xlf --i18nFormat=xlf --locale=es --missingTranslation=warning & node app.js ",
    "start:fr": "ng serve --aot --i18nFile=src/client/i18n/texts.fr.xlf --i18nFormat=xlf --locale=fr --missingTranslation=warning & node app.js ",
    "test-client": "ng test",
    "test-server": "gulp test",
    "translate": "ng xi18n --outputPath src/client/i18n --outFile texts.xlf ng xi18n --locale es",
    "tslint": "tslint \"src/**/*.ts\" --project src/client/tsconfig.app.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.e2e.json --type-check"
  },
